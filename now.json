{
  "version": 2,
  "env": {
    "DOC_API_KEY": "@doc-api-key"
  },
  "builds": [
    {
      "src": "README.md",
      "use": "@now/static"
    },
    { "src": "index.js", "use": "@now/node-server" }
  ],
  "routes": [
    {
      "src": "/cases",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/cases-outside-ph",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/test-results",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/patients-under-investigation",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/mm-checkpoints",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/mm-checkpoints/(?<id>[^/]*)",
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ]
}
